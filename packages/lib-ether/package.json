        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      },
      "./*": "./*"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "nr build",
    "start": "tsx src/index.ts"
  },
  "peerDependencies": {
    "ethers": ">=6"
  },
  "dependencies": {
    "@hairy/utils": "workspace:*",
    "mitt": "catalog:utils"
  },
  "devDependencies": {
    "ethers": "catalog:inlined"
  }
}
